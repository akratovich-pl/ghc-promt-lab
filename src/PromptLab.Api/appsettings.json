{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=promptlab.db"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:5173"]
  },
  "LlmProviders": {
    "GoogleGemini": {
      "Enabled": true,
      "BaseUrl": "https://generativelanguage.googleapis.com/v1beta",
      "DefaultModel": "gemini-pro",
      "MaxTokens": 8192,
      "Temperature": 0.7
    }
  }
}
